{
    "$schema" : "http://json-schema.org/draft-03/schema",
    "title" : "mesh",
    "type" : "object",
    "description" : "mesh",
    "properties" : {
        "name" : {
            "type" : "string",
            "description" : "The artist-created name of this mesh.  This is not necessarily globally unique."
        },
        "primitives" : {
            "type" : "array",
            "description" : "Primitives that form the geometry in this mesh.",
            "items" : {
                "$ref" : "meshPrimitive.schema.json"
            }
        },
        "extra" : {
            "type" : "any",
            "description" : "Optional application-specific data."
        }
    },
    "additionalProperties" : false
}

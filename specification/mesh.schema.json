{
    "$schema" : "http://json-schema.org/draft-03/schema",
    "title" : "mesh",
    "type" : "object",
    "description" : "An array of primitives defining geometry and material.",
    "properties" : {
        "name" : {
            "type" : "string",
            "description" : "The user-defined name of this mesh.  This is not necessarily unique among meshes."
        },
        "primitives" : {
            "type" : "array",
            "description" : "Primitives that define geometry and material.",
            "items" : {
                "$ref" : "meshPrimitive.schema.json"
            }
        },
        "extensions" : {
            "properties" : {
            },
            "additionalProperties" : {
                "$ref" : "extension.schema.json"
            }
        },
        "extras" : {
            "$ref" : "extra.schema.json"
        }
    },
    "additionalProperties" : false
}

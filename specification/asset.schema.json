{
    "$schema" : "http://json-schema.org/draft-03/schema",
    "title" : "asset",
    "type" : "object",
    "description" : "Asset-management information for this model.",
    "properties" : {
        "copyright" : {
            "type" : "string",
            "description" : "A copyright message suitable for display to credit the asset author."
        },
        "generator" : {
            "type" : "string",
            "description" : "Tool that generated this glTF asset.  Useful for debugging."
        },
        "premultipliedAlpha" : {
            "type" : "boolean",
            "description" : "getContext() with premultipliedAlpha",
            "default" : false
        },
        "extensions" : {
            "properties" : {
            },
            "additionalProperties" : {
                "$ref" : "extension.schema.json"
            }        
        },
        "extras" : {
            "type" : "any",
            "description" : "Optional application-specific data."
        }
    },
    "additionalProperties" : false
}
